{
    "name": "aequation/toolwire",
    "type": "symfony-bundle",
    "description": "This bundle is my symfony common base for my projects.",
    "keywords": [ "symfony","symfony-bundle" ],
    "homepage": "https://www.aequation.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Dujardin / Aequation",
            "homepage": "https://github.com/manu7772"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aequation\\ToolwireBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aequation\\ToolwireBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "symfony/config": "7.2.*",
        "symfony/dependency-injection": "7.2.*",
        "symfony/stimulus-bundle": "^2.22",
        "symfony/http-kernel": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/filesystem": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/string": "7.2.*",
        "symfony/yaml": "7.2.*",
        "doctrine/orm": "^3.3",
        "symfony/serializer": "7.2.*",
        "symfony/validator": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/form": "7.2.*",
        "twig/twig": "^2.12|^3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "symfony/translation": "7.2.*",
        "symfony/event-dispatcher": "7.2.*",
        "symfonycasts/dynamic-forms": "^0.1.3",
        "symfony/ux-twig-component": "^2.22",
        "symfony/ux-live-component": "^2.22",
        "symfony/ux-icons": "^2.22",
        "symfony/ux-leaflet-map": "^2.22",
        "knplabs/knp-paginator-bundle": "^6.6",
        "nucleos/dompdf-bundle": "^4.3",
        "aequation/tools": "dev-main"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true,
                "versions": {
                    "aequation/*": "dev-main"
                }
            },
            "canonical": false,
            "only": ["aequation/*"],
            "fallback": true
        }
    ],
    "require-dev": {
        "symfony/framework-bundle": "7.2.*",
        "symfony/phpunit-bridge": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/var-dumper": "7.2.*",
        "symfony/stopwatch": "7.2.*",
        "symfony/maker-bundle": "^1.61"
    },
    "conflict": {
        "symfony/flex": "<1.13"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    },
    "extra": {
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "minimum-stability": "stable"
}
